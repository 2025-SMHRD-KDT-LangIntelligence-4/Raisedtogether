<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.smhrd.web.mapper.KakaomapMapper">
	<select id="selectMapByParentId" parameterType="String" resultType="Parent">
		select parent_id, parent_lat, parent_lng
		from t_parent
		where parent_id = #{parentId}
	</select>
</mapper>